#lang sicp

(define (A x y)
  (cond ((= y 0) 0)
        ((= x 0) (* 2 y))
        ((= y 1) 2)
        (else (A (- x 1)
                 (A x (- y 1))))))

;;;(f n) = 2n

(A 1 10)
(A 2 4)
(A 3 3)

(A 1 2) ;;;4
(A 1 3) ;;;8
(A 1 4) ;;;16
(A 1 5) ;;;32
;;;(g n) = 2^n

(A 2 1)
(A 2 2)
(A 2 3)
(A 2 4)

